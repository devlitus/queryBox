---
import KeyValueTable from "../shared/KeyValueTable.astro";
import { mockRequest } from "../../data/mock-request";

const headers = mockRequest.headers.map((h) => ({
	key: h.key,
	value: h.value,
	description: h.description,
	enabled: h.enabled,
}));
---

<KeyValueTable items={headers} showDescription={false} />
